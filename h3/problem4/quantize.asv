function [] = quantize(dG)
    

    % get all the dc term elements
    size(dG)
    
    m = 640 / 8;
    n = 832 / 8;
    
    rowDim = zeros(m, 1);
    for i = 1:m
        rowDim(i) = 8;
    end
    colDim = zeros(n, 1);
    for i = 1:n
        colDim(i) = 8;
    end

    % split the matrix into 8*8 block matrix 
    dG2 = mat2cell(dG, rowDim, colDim);
    
    count = 1;
    dcTerm = zeros(m*n, 1);
    
    elementIn14 = zeros(m*n*14, 1);
    cn = 1;


end